
quad: { :name
	a:0, b:0, c:0
	discriminant: b^2 - 4*a*c
	rooted: sqrt { x: discriminant }
	denominator: 2*a

	neg: (-b - rooted) / denominator
	pos: (-b + rooted) / denominator
}

result: Math.operation { a + b }

result: Sqrt { x: a + b }


